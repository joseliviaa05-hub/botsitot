{
  "name": "bot-whatsapp-gratis",
  "version": "2.0.0",
  "description": "Bot gratuito de WhatsApp para negocio multirubro - Refactor v2 con TypeScript",
  "main": "dist/index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only --ignore-watch node_modules src/index.ts",
    "dev:debug": "ts-node-dev --respawn --transpile-only --inspect -- src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "jest --runInBand",
    "test:watch": "jest --watch --runInBand",
    "test:coverage": "jest --coverage --runInBand",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:deploy": "prisma migrate deploy",
    "prisma:studio": "prisma studio",
    "prisma:seed": "ts-node -r tsconfig-paths/register prisma/seed.ts",
    "db:push": "prisma db push",
    "db:reset": "prisma migrate reset --force",
    "check-data": "ts-node src/check-data.ts",
    "test-indexes": "ts-node src/test-indexes.ts",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\" \"prisma/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\" \"prisma/**/*.ts\"",
    "type-check": "tsc --noEmit",
    "prepare": "husky"
  },
  "keywords": [
    "whatsapp",
    "bot",
    "gratis",
    "baileys",
    "negocio",
    "typescript",
    "prisma",
    "redis",
    "neon",
    "express",
    "docker"
  ],
  "author": "joseliviaa05-hub",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^6.19.0",
    "@sentry/node": "^10.29.0",
    "@sentry/profiling-node": "^10.29.0",
    "@upstash/redis": "^1.35.7",
    "@whiskeysockets/baileys": "^6.7.0",
    "async-lock": "^1.4.1",
    "bcryptjs": "^3.0.3",
    "bull": "^4.16.5",
    "cloudinary": "^2.8.0",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^4.22.1",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^8.2.1",
    "express-validator": "^7.3.1",
    "groq-sdk": "^0.7.0",
    "helmet": "^8.1.0",
    "hpp": "^0.2.3",
    "ioredis": "^5.8.2",
    "joi": "^18.0.2",
    "jsonwebtoken": "9.0.2",
    "multer": "^2.0.2",
    "pino": "^8.19.0",
    "pino-pretty": "^11.0.0",
    "qrcode-terminal": "^0.12.0",
    "rate-limit-redis": "^4.3.1",
    "redis": "^5.10.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "whatsapp-web.js": "^1.34.2",
    "winston": "^3.19.0",
    "winston-daily-rotate-file": "^5.0.0",
    "xss-clean": "^0.1.4"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/bull": "^3.15.9",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/express-mongo-sanitize": "^1.3.2",
    "@types/hpp": "^0.2.7",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "9.0.6",
    "@types/multer": "^1.4.12",
    "@types/node": "^24.10.1",
    "@types/qrcode-terminal": "^0.12.2",
    "@types/redis": "^4.0.10",
    "@types/supertest": "^6.0.3",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "@typescript-eslint/eslint-plugin": "^8.48.1",
    "@typescript-eslint/parser": "^8.48.1",
    "dotenv-cli": "^11.0.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "husky": "^9.1.7",
    "jest": "^30.2.0",
    "lint-staged": "^15.5.2",
    "nodemon": "^3.1.11",
    "prettier": "^3.7.4",
    "prisma": "^6.19.0",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "prisma": {
    "seed": "ts-node -r tsconfig-paths/register prisma/seed.ts"
  },
  "lint-staged": {
    "src/**/*.ts": [
      "eslint --fix",
      "prettier --write"
    ],
    "prisma/**/*.prisma": [
      "prettier --write"
    ]
  }
}
